{
  "name": "11ty-resume-template",
  "version": "1.0.1",
  "description": "This template can be used to create new resumes with 11ty.",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all -p dev:*",
    "build": "run-s build:*",
    "dev:11ty": "eleventy --serve",
    "dev:css": "npx tailwindcss@3 -i src/assets/css/tailwind.css -o _site/assets/css/styles.css --watch",
    "build:11ty": "eleventy",
    "build:css": "npx tailwindcss -i src/assets/css/tailwind.css -o _site/assets/css/styles.css"
  },
  "keywords": [
    "template",
    "11ty",
    "eleventy",
    "resume",
    "cv",
    "css",
    "tailwindcss",
    "postcss",
    "html",
    "javascript",
    "nodejs",
    "npm",
    "github"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "autoprefixer": "^10.4.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.19"
  },
  "dependencies": {
    "netlify-cli": "^23.12.3"
  },
  "overrides": {
    "jws": "^4.0.1",
    "@octokit/endpoint": "^10.1.1",
    "@octokit/request": "^9.1.3",
    "@octokit/request-error": "^6.1.5",
    "brace-expansion": "^2.0.2",
    "esbuild": "^0.24.3",
    "glob": "^11.0.0",
    "nanoid": "^3.3.8",
    "node-forge": "^1.4.0",
    "on-headers": "^1.1.0",
    "tar-fs": "^3.1.1",
    "tmp": "^0.2.4"
  }
}